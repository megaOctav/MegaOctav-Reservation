        "/api/transactions": {
            "get": {
                "tags": [
                    "Transactions"
                ],
                "summary": "Ambil semua transaksi",
                "operationId": "5effd5c228c8c92f09b136d74aacab83",
                "responses": {
                    "200": {
                        "description": "Berhasil mengambil daftar transaksi",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer"
                                            },
                                            "id_user": {
                                                "type": "integer"
                                            },
                                            "schedule_id": {
                                                "type": "integer"
                                            },
                                            "payment_method": {
                                                "type": "string"
                                            },
                                            "total_price": {
                                                "type": "number"
                                            },
                                            "payment_date": {
                                                "type": "string",
                                                "format": "date"
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "format": "date-time"
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "format": "date-time"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Transactions"
                ],
                "summary": "Buat transaksi baru",
                "operationId": "f5614e1a4ee60e20e9c82334e2c7e5be",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "id_user",
                                    "schedule_id",
                                    "payment_method",
                                    "total_price",
                                    "payment_date"
                                ],
                                "properties": {
                                    "id_user": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "schedule_id": {
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "payment_method": {
                                        "type": "string",
                                        "example": "gopay"
                                    },
                                    "total_price": {
                                        "type": "number",
                                        "example": 100000
                                    },
                                    "payment_date": {
                                        "type": "string",
                                        "format": "date",
                                        "example": "2025-05-21"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Transaksi berhasil dibuat",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": 5
                                        },
                                        "id_user": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "schedule_id": {
                                            "type": "integer",
                                            "example": 2
                                        },
                                        "payment_method": {
                                            "type": "string",
                                            "example": "gopay"
                                        },
                                        "total_price": {
                                            "type": "number",
                                            "example": 100000
                                        },
                                        "payment_date": {
                                            "type": "string",
                                            "format": "date",
                                            "example": "2025-05-21"
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2025-05-22T10:00:00Z"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2025-05-22T10:00:00Z"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    }
                }
            }
        },
